<h1>All Products</h1>
<a href="/admin/products/new">Add New Product</a>
<ul>
  <% products.forEach(p => { %>
    <li>
      <%= p.name %> - $<%= p.price %>
      <a href="/admin/products/edit/<%= p._id %>">Edit</a>
      <form method="DELETE" action="/admin/products/<%= p._id %>/delete" style="display:inline;">
        <button type="submit">Delete</button>
      </form>
    </li>
  <% }) %>
</ul>
